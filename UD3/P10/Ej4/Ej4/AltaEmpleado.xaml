<Window x:Class="Ej4.AltaEmpleado"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ej4"
        mc:Ignorable="d"
        Title="AltaEmpleado" Height="250" Width="300">
    <StackPanel HorizontalAlignment="Center">
        <TextBlock Text="Nombre:" HorizontalAlignment="Center"/>
        <TextBox Name="txtNombre" Width="200" Margin="10"/>        
        <TextBlock Text="Salario:" HorizontalAlignment="Center"/>
        <TextBox Name="txtSalario" Width="200" Margin="10"/>
        <Button Content="Enviar" Click="Enviar" Width="80" Margin="10"/>
        <Button Content="Cancelar" Click="Cancelar" Width="80" Margin="10"/>
    </StackPanel>
</Window>
