<Window x:Class="Ej1.SegundaVentana"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ej1"
        mc:Ignorable="d"
        Title="SegundaVentana" Height="250" Width="300">
    <StackPanel>
        <Label Content="Email" Margin="6" VerticalAlignment="Top"/>
        <TextBox Width="250" Height="20" HorizontalAlignment="Left" Margin="6" Name="txtEmail"/>
        <Button Name="btnEnviar" Content="Enviar" Click="EnviarEmail" Width="80" HorizontalAlignment="Left" Margin="6"/>
    </StackPanel>
</Window>
